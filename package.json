{
  "name": "kaiware",
  "productName": "kaiware",
  "version": "0.1.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "prettier:check": "prettier -c src *.md *.js *.json",
    "prettier:fix": "prettier -w src *.md *.js *.json",
    "typescript:check": "tsc --noEmit",
    "test": "npm run typescript:check && npm run lint && npm run prettier:check"
  },
  "keywords": [],
  "author": {
    "name": "Garrett Downs",
    "email": "garredow@outlook.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/static/images/icon/icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Kaiware"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "maintainer": "Garrett Downs",
            "homepage": "https://github.com/garredow/kaiware"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "maintainer": "Garrett Downs",
            "homepage": "https://github.com/garredow/kaiware"
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-dmg": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.61",
    "@types/follow-redirects": "^1.13.1",
    "@types/lodash": "^4.14.177",
    "@types/lunr": "^2.3.4",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/util.promisify": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "electron": "16.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "node-loader": "^2.0.0",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@cliqz-oss/firefox-client": "0.3.1",
    "date-fns": "^2.27.0",
    "electron-dl": "^3.3.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "extract-zip": "^2.0.1",
    "follow-redirects": "^1.14.5",
    "lodash": "^4.17.21",
    "lunr": "^2.3.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.0",
    "react-router-dom": "^5.3.0",
    "util.promisify": "^1.1.1"
  }
}
